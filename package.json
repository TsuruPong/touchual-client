{
  "name": "touchual-client",
  "private": true,
  "type": "module",
  "scripts": {
    "build-dev": "dotenv -e .env.dev react-router build",
    "build-prod": "dotenv -e .env.prod react-router build",
    "deploy-dev": "aws s3 sync build/client s3://touchual-dev-client --delete",
    "deploy-prod": "aws s3 sync build/client s3://touchual-prod-client --delete",
    "dev": "dotenv -e .env.dev react-router dev",
    "zip": "bestzip client.zip build/ node_modules/",
    "packing": "npm-run-all --serial build zip",
    "start": "dotenv -e .env.prod react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "isbot": "^5.1.27",
    "manimani": "^1.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "sass": "^1.88.0",
    "swr": "^2.3.3",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@react-router/dev": "^7.5.3",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "bestzip": "^2.2.1",
    "dotenv-cli": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.52.0",
    "storybook": "^8.6.12",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  }
}
